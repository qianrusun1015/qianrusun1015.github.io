############################################################################
############################################################################
### hole = 0
layer {
  name: "fc6_1"
  type: "Convolution"
  bottom: "pool5"
  top: "fc6_1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 1024
    pad: 1
    kernel_size: 3
    #dilation: 6
  }
}
layer {
  name: "relu6_1"
  type: "ReLU"
  bottom: "fc6_1"
  top: "fc6_1"
}
layer {
  bottom: "fc6_1"
  top: "pool_cam1"
  name: "pool_cam1"
  type: "Pooling"
  pooling_param {
    pool: AVE
    kernel_size: 41
    stride: 1
  }
}
layer {
  name: "drop6_1"
  type: "Dropout"
  bottom: "pool_cam1"
  top: "pool_cam1"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  bottom: "pool_cam1"
  top: "fc_cam1"
  name: "fc_cam1"
  type: "InnerProduct"
  param { lr_mult: 10 decay_mult: 1 }
  inner_product_param {
    bias_term: false
    num_output: 20
    weight_filler { type: "gaussian"  std: 0.005 }
  }
}
layer {
  name: "loss_s1"
  type: "SigmoidCrossEntropyLoss"
  bottom: "fc_cam1"
  bottom: "label"
  top: "loss_s1"
}

